<VirtualHost *>
    ServerName generator.mycorp.dyndns.org
    ServerAdmin webmaster@localhost

    DocumentRoot /var/www/apache/generator.mycorp.dyndns.org/public
    <Directory /var/www/apache/generator.mycorp.dyndns.org/public/>
        Options FollowSymLinks
        AllowOverride All
    </Directory>

    LogLevel warn
    ErrorLog ${APACHE_LOG_DIR}/error.log
    CustomLog ${APACHE_LOG_DIR}/access.log combined
</VirtualHost>
